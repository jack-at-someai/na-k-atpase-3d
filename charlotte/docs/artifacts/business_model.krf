;;; ================================================================
;;; SOMEAI BUSINESS MODEL SUBSTRATE
;;; Revenue streams, client pipeline, and onboarding engine
;;; encoded in Charlotte's five primitives
;;; ================================================================

(in-microtheory SomeAIBusinessModelMt)

;;; ── VOCABULARY ──────────────────────────────────────────────────

;; Node types

(isa Entity Collection)
(genls Entity NODE)
(comment Entity "A legal entity in the SomeAI business graph")

(isa OperatingCompany Collection)
(genls OperatingCompany Entity)
(comment OperatingCompany "A company SomeAI operates or wholly owns")

(isa Client Collection)
(genls Client Entity)
(comment Client "A business with an active or drafted managed service contract")

(isa PartnerRegistry Collection)
(genls PartnerRegistry Entity)
(comment PartnerRegistry "An industry registry or association in a revenue-sharing partnership")

(isa ConsumerProduct Collection)
(genls ConsumerProduct NODE)
(comment ConsumerProduct "A consumer-facing app generating App Store / subscription revenue")

(isa RevenueStream Collection)
(genls RevenueStream NODE)
(comment RevenueStream "A named source of recurring revenue")

(isa Industry Collection)
(genls Industry NODE)
(comment Industry "An industry vertical Charlotte deploys into")

(isa ContractPhase Collection)
(genls ContractPhase NODE)
(comment ContractPhase "A phase of the double-contract model (Foundation or Operations)")

;; Edge types

(isa hasContract Predicate)
(arity hasContract 2)
(arg1Isa hasContract Entity)
(arg2Isa hasContract ContractPhase)
(comment hasContract "Links an entity to a contract phase")

(isa operatesIn Predicate)
(arity operatesIn 2)
(arg1Isa operatesIn Entity)
(arg2Isa operatesIn Industry)
(comment operatesIn "Links an entity to its industry vertical")

(isa owns Predicate)
(arity owns 2)
(arg1Isa owns Entity)
(arg2Isa owns Entity)
(comment owns "Ownership relationship between entities")

(isa isClientOf Predicate)
(arity isClientOf 2)
(arg1Isa isClientOf Entity)
(arg2Isa isClientOf Entity)
(comment isClientOf "Managed service client relationship")

(isa isPartnerOf Predicate)
(arity isPartnerOf 2)
(arg1Isa isPartnerOf Entity)
(arg2Isa isPartnerOf Entity)
(comment isPartnerOf "Revenue-sharing partnership relationship")

(isa generates Predicate)
(arity generates 2)
(arg1Isa generates NODE)
(arg2Isa generates RevenueStream)
(comment generates "Links a product or client to a revenue stream")

(isa deploysThrough Predicate)
(arity deploysThrough 2)
(arg1Isa deploysThrough Entity)
(arg2Isa deploysThrough Entity)
(comment deploysThrough "Charlotte deploys into an industry through this entity")

;; Metric types

(isa ContractValue Collection)
(genls ContractValue METRIC)
(comment ContractValue "Total 2-year contract value in USD")

(isa MonthlyRetainer Collection)
(genls MonthlyRetainer METRIC)
(comment MonthlyRetainer "Monthly ongoing service fee in USD")

(isa AnnualRevenue Collection)
(genls AnnualRevenue METRIC)
(comment AnnualRevenue "Estimated annual revenue of a client entity")

(isa GrossMargin Collection)
(genls GrossMargin METRIC)
(comment GrossMargin "Gross margin percentage for a client engagement")

(isa LifetimeValue Collection)
(genls LifetimeValue METRIC)
(comment LifetimeValue "Estimated 5-year lifetime value of a client relationship")

(isa PipelineStage Collection)
(genls PipelineStage METRIC)
(comment PipelineStage "Current stage in the sales pipeline (0-5)")

(isa DataMaturity Collection)
(genls DataMaturity METRIC)
(comment DataMaturity "Client data maturity assessment (0-5 scale)")

(isa ProjectedARR Collection)
(genls ProjectedARR METRIC)
(comment ProjectedARR "Projected annual recurring revenue by year")

;; Signal types

(isa ContractEvent Collection)
(genls ContractEvent SIGNAL)
(comment ContractEvent "A timestamped event in the contract lifecycle")

(isa RevenueSignal Collection)
(genls RevenueSignal SIGNAL)
(comment RevenueSignal "A timestamped revenue observation (invoice, payment, MRR)")

(isa OnboardingSignal Collection)
(genls OnboardingSignal SIGNAL)
(comment OnboardingSignal "A timestamped event in the client onboarding process")

;; Protocol types

(isa OnboardingProtocol Collection)
(genls OnboardingProtocol PROTOCOL)
(comment OnboardingProtocol "The 5-wave onboarding engine for new clients")

(isa DoubleContractProtocol Collection)
(genls DoubleContractProtocol PROTOCOL)
(comment DoubleContractProtocol "Foundation Phase → Operations Phase contract structure")

(isa PricingProtocol Collection)
(genls PricingProtocol PROTOCOL)
(comment PricingProtocol "Value-based pricing rules by client tier")

