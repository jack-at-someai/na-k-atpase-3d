;;; ── PROTOCOLS ───────────────────────────────────────────────────

;; Double Contract Protocol
(isa DoubleContract DoubleContractProtocol)
(comment DoubleContract "Every managed service client gets two contracts.")

(isa FoundationPhase ContractPhase)
(comment FoundationPhase "Contract A: Deployment & migration. 3-6 months. One-time.")

(isa OperationsPhase ContractPhase)
(comment OperationsPhase "Contract B: Ongoing managed service. 18-24 months. Monthly retainer.")

;; Foundation Phase steps
(hasStep DoubleContract 1 "Discovery: Domain walkthrough with operator. 8-16 hours.")
(hasStep DoubleContract 2 "Vocabulary Configuration: Define NODEs, EDGEs, METRICs, PROTOCOLs. 12-24 hours.")
(hasStep DoubleContract 3 "Historical Import: Ingest existing records into graph. 16-40 hours.")
(hasStep DoubleContract 4 "Graph Seeding: Connect to shared substrate. 8-16 hours.")
(hasStep DoubleContract 5 "Protocol Calibration: Build expected trajectories. 8-20 hours.")
(hasStep DoubleContract 6 "Validation & Handoff: Verify graph integrity. 8-12 hours.")

;; Onboarding Protocol (5-wave execution engine from playbook)
(isa ClientOnboarding OnboardingProtocol)
(comment ClientOnboarding "5-wave parallel execution engine for spinning up a new client from zero.")

(hasStep ClientOnboarding 1 "Wave 1: Research & Analysis — Business Analysis, Competitive Analysis, Cost Analysis, Capital Assessment. Parallel, no dependencies.")
(hasStep ClientOnboarding 2 "Wave 2: Substrate & Brand — KRF substrate, Brand Book, Design System. Parallel, depends on Wave 1.")
(hasStep ClientOnboarding 3 "Wave 3: PRD & Design — Product Requirements Doc, Software Design Doc. Depends on Wave 1+2.")
(hasStep ClientOnboarding 4 "Wave 4: Pitch & Contracts — Pitch Deck, MSA (2 contracts). Depends on Wave 1.")
(hasStep ClientOnboarding 5 "Wave 5: Dashboard & Visualization — Client dashboard on index page. Depends on Wave 2+3.")

;; Pricing Protocol
(isa ValueBasedPricing PricingProtocol)
(comment ValueBasedPricing "Price at 15-35% of quantified value created. Never cost-plus.")

(hasPricingTier ValueBasedPricing "Operator" 3000 8000)
(hasPricingTier ValueBasedPricing "Enterprise" 12000 35000)
(hasPricingTier ValueBasedPricing "Portfolio" 50000 150000)

;; Pipeline Stage Protocol
(isa PipelineStageProtocol PROTOCOL)
(comment PipelineStageProtocol "Pipeline stages for client acquisition.")
(hasStep PipelineStageProtocol 0 "Identified: Business exists, no interaction yet.")
(hasStep PipelineStageProtocol 1 "Researched: Competitive analysis and business analysis complete.")
(hasStep PipelineStageProtocol 2 "Engaged: In discussion, deliverables in progress.")
(hasStep PipelineStageProtocol 3 "Proposed: MSA drafted, full onboarding package ready.")
(hasStep PipelineStageProtocol 4 "Committed: MSA signed, awaiting deployment start date.")
(hasStep PipelineStageProtocol 5 "Active: Contract live, service delivery in progress.")

