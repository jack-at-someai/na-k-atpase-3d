;;; ================================================================
;;; CHARLOTTE TEMPORAL SUBSTRATE - COMPLETE ONTOLOGY
;;; 15 granularity levels | 58 categorical nodes | 4 temporal directions
;;; Range: 0-2048 CE | ~22.6M nodes at hour resolution
;;; ================================================================

(in-microtheory CharlotteTemporalMt)

;;; ================================================================
;;; SECTION 1: TEMPORAL DIRECTIONS (The Temporal Compass)
;;; ================================================================

(isa TemporalDirection Collection)
(genls TemporalDirection NODE)
(comment TemporalDirection "The four directions of temporal navigation. Analogous to cardinal directions in spatial substrate.")

(isa Coarser TemporalDirection)
(comment Coarser "Toward lower resolution: Day to Month to Year. Zoom out.")

(isa Finer TemporalDirection)
(comment Finer "Toward higher resolution: Year to Month to Day. Zoom in.")

(isa Earlier TemporalDirection)
(comment Earlier "Backward in sequence. Traversing prev edges.")

(isa Later TemporalDirection)
(comment Later "Forward in sequence. Traversing next edges.")

;;; ================================================================
;;; SECTION 2: GRANULARITY LEVELS (Meta-layer)
;;; ================================================================

(isa TemporalGranularity Collection)
(genls TemporalGranularity NODE)
(comment TemporalGranularity "A level in the temporal resolution hierarchy. 15 levels from Millennium to Hour.")

;;; --- Granularity Predicates ---

(isa granularityLevel Predicate)
(arity granularityLevel 2)
(arg1Isa granularityLevel TemporalGranularity)
(arg2Isa granularityLevel Thing)
(comment granularityLevel "Ordinal: 1=Millennium (coarsest) to 15=Hour (finest)")

(isa typicalDurationDays Predicate)
(arity typicalDurationDays 2)
(arg1Isa typicalDurationDays TemporalGranularity)
(arg2Isa typicalDurationDays Thing)
(comment typicalDurationDays "Approximate days per unit")

(isa substrateNodeCount Predicate)
(arity substrateNodeCount 2)
(arg1Isa substrateNodeCount TemporalGranularity)
(arg2Isa substrateNodeCount Thing)
(comment substrateNodeCount "Instance nodes for 0-2048 CE")

(isa granularityDirectionTo Predicate)
(arity granularityDirectionTo 3)
(arg1Isa granularityDirectionTo TemporalGranularity)
(arg2Isa granularityDirectionTo TemporalGranularity)
(arg3Isa granularityDirectionTo TemporalDirection)
(comment granularityDirectionTo "From level A, direction D reaches level B. Analogous to cardinalDirectionTo in spatial substrate.")

(isa adjacentGranularity Predicate)
(arity adjacentGranularity 2)
(arg1Isa adjacentGranularity TemporalGranularity)
(arg2Isa adjacentGranularity TemporalGranularity)
(comment adjacentGranularity "These levels are neighbors in the hierarchy. Analogous to adjacentTo.")

(isa containsPerUnit Predicate)
(arity containsPerUnit 3)
(arg1Isa containsPerUnit TemporalGranularity)
(arg2Isa containsPerUnit TemporalGranularity)
(arg3Isa containsPerUnit Thing)
(comment containsPerUnit "One unit of A contains ~N units of B")

