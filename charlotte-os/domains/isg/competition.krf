;;; ─── Predicates ─────────────────────────────────────────────────────────

(isa competitorOf Predicate)
(arity competitorOf 2)
(arg1Isa competitorOf IndustrialServiceCompany)
(arg2Isa competitorOf IndustrialServiceCompany)

(isa competitorTier Predicate)
(arity competitorTier 2)
(arg1Isa competitorTier IndustrialServiceCompany)
(arg2Isa competitorTier CompetitorTier)

(isa threatLevel Predicate)
(arity threatLevel 3)
(arg1Isa threatLevel IndustrialServiceCompany)
(arg2Isa threatLevel IndustrialServiceCompany)
(arg3Isa threatLevel CompetitiveThreatLevel)
(comment threatLevel "(threatLevel ?competitor ?target ?level) — threat of ?competitor to ?target")

(isa operatesInVertical Predicate)
(arity operatesInVertical 2)
(arg1Isa operatesInVertical IndustrialServiceCompany)
(arg2Isa operatesInVertical IndustrialVertical)

(isa annualRevenue Predicate)
(arity annualRevenue 2)
(comment annualRevenue "Revenue in USD millions")

(isa locationCount Predicate)
(arity locationCount 2)

(isa employeeCount Predicate)
(arity employeeCount 2)

(isa skuCount Predicate)
(arity skuCount 2)

(isa headquarteredIn Predicate)
(arity headquarteredIn 2)

(isa marketSharePct Predicate)
(arity marketSharePct 2)

(isa geographicOverlap Predicate)
(arity geographicOverlap 3)
(comment geographicOverlap "(geographicOverlap ?competitor ISG ?level)")

(isa pricingBenchmark Predicate)
(arity pricingBenchmark 4)
(comment pricingBenchmark "(pricingBenchmark ?vertical ?metric ?lowValue ?highValue)")

(isa marketSize Predicate)
(arity marketSize 3)
(comment marketSize "(marketSize ?vertical ?year ?valueUSDMillions)")

(isa marketGrowthRate Predicate)
(arity marketGrowthRate 2)
(comment marketGrowthRate "(marketGrowthRate ?vertical ?cagrPct)")

(isa grossMarginPct Predicate)
(arity grossMarginPct 2)

(isa partsMarkupPct Predicate)
(arity partsMarkupPct 3)
(comment partsMarkupPct "(partsMarkupPct ?company ?low ?high)")

